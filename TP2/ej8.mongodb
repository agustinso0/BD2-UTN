/*
Ejercicio 8: Uso de $lookup Realiza una agregaci√≥n donde se combinen los datos de alumnos y cursos usando $lookup.
*/
use("universidad")

db.alumnos.aggregate([
  {
    $lookup: {
      from: "cursos",           
      localField: "id_cursos",  
      foreignField: "_id",     
      as: "inscripto_a"  
    }
  }
])
